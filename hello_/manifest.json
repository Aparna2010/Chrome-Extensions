{
  "name": "Hello Extensions",
  "description" : "Base Level Extension-Opens html popup using key shortcut",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Opens popup.html"

    }
  }
}
